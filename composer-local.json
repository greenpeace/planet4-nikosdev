{
	"name": "greenpeace/planet4-nikosdev",
	"description": "Greenpeace P4 NikosDev",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-plugin-gutenberg-blocks" : "dev-master",
		"greenpeace/planet4-master-theme" : "dev-master"
	},
	"data_from": "international",
	"scripts": {
		"clone:ghkey": "cat ${GITHUB_RO} > .key",
		"clone:3rdparty": "GIT_SSH_COMMAND='ssh -i .key' git clone --depth=1 git@github.com:greenpeace/planet4-3rdparty-plugins.git",
		"install:plugin-wpml": "wp plugin install --activate planet4-3rdparty-plugins/sitepress-multilingual-cms.4.3.4.zip",
    		"site:custom": [
			"@clone:ghkey",
			"@clone:3rdparty",
      			"@install:plugin-wpml"
		]
	}
}
